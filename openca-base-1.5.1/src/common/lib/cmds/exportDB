##   File Name: exportDB
##     Version: $Revision: 1.1.1.1 $
##       Brief: Exports DB
## Description: Exports on a removable media all

use strict;

sub cmdExportDB {

	print startLogPage ( gettext("Exporting DB ...") );

	exportDB ();

	closePage ();

}

1;

