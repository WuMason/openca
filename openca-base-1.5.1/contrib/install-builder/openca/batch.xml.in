        <component>
            <name>batch</name>
            <description>Batch Processor</description>
            <canBeEdited>1</canBeEdited>
            <selected>1</selected>
            <show>1</show>
            <folderList>
                <folder>
                    <description>Batch Interface Conf</description>
                    <destination>${installdir}/etc/openca</destination>
                    <name>bp</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>@build_dir@@etc_prefix@/bp</origin>
                        </distributionDirectory>
		    </distributionFileList>
		</folder>
                <folder>
                    <description>Batch Interface</description>
                    <destination>@cgi_fs_prefix@</destination>
                    <name>batchcgi</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>@build_dir@@batch_cgi_fs_prefix@</origin>
                        </distributionDirectory>
		    </distributionFileList>
		</folder>
                <folder>
                    <description>Batch Interface</description>
                    <destination>@htdocs_fs_prefix@</destination>
                    <name>batchhtml</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>@build_dir@@batch_htdocs_fs_prefix@</origin>
                        </distributionDirectory>
		    </distributionFileList>
		</folder>
		<folder>
                    <description>Batch Interface Access Control</description>
                    <destination>${installdir}/etc/openca/access_control</destination>
                    <name>batchac</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>@build_dir@@etc_prefix@/access_control/batch.xml.template</origin>
                        </distributionFile>
		    </distributionFileList>
		    <actionList>
			<changeOwnerAndGroup>
			    <owner>@httpd_user@</owner>
			    <group>@httpd_group@</group>
			    <files>@build_dir/@etc_prefix@/access_control/batch.*</files>
			</changeOwnerAndGroup>
		   </actionList>
		</folder>
                <folder>
                    <description>Batch Menu CFG</description>
                    <destination>${installdir}/etc/openca/menus</destination>
                    <name>batchmenuconfig</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>@build_dir@@etc_prefix@/menus/batch-menu.xml.template</origin>
                        </distributionFile>
                    </distributionFileList>
                </folder>
		<folder>
                    <description>Batch Interface Server Config</description>
                    <destination>${installdir}/etc/openca/servers</destination>
                    <name>batchsrv</name>
                    <platforms>all</platforms>
                    <distributionFileList>
                        <distributionFile>
                            <origin>@build_dir@@etc_prefix@/servers/batch.conf.template</origin>
                        </distributionFile>
                    </distributionFileList>
		    <actionList>
			<changeOwnerAndGroup>
			    <owner>@httpd_user@</owner>
			    <group>@httpd_group@</group>
			    <files>@build_dir@@etc_prefix@/servers/*</files>
			</changeOwnerAndGroup>
		   </actionList>
                </folder>
            </folderList>
        </component>
